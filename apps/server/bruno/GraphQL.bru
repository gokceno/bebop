meta {
  name: GraphQL
  type: graphql
  seq: 2
}

post {
  url: {{BASE_URL}}/graphql
  body: graphql
  auth: inherit
}

body:graphql {
  query {
    events(
      where: {
        eventName: { in: ["bookingInfoSentToCustomerBySms"] }
        paramsFlat: { customerId: { eq: "customer_12345" } }
        claims: { email: { eq: "gokcen@brewww.com" } }
      }
      limit: 2
      order: "desc"
    ) {
      events {
        id
        eventName
        createdAt
        claims {
          claimName
          claimValue
        }
        params {
          paramName
          paramValue
        }
        traces {
          traceData
        }
      }
      total
    }
  }
  
}
