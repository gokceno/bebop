meta {
  name: GraphQL - Param Operators
  type: graphql
  seq: 6
}

post {
  url: {{BASE_URL}}/graphql
  body: graphql
  auth: inherit
}

body:graphql {
  query {
    events(
      where: {
        eventName: { eq: "monthlyPriceEntered" },
        params: {
          monthlyPriceEntered: {
            propertyId: { eq: "property_12345" }
          }
        }
      },
      limit: 10,
      order: "desc"
    ) {
      events {
        id
        eventName
        createdAt
        params {
          paramName
          paramValue
        }
        traces {
          traceData
        }
      }
      total
    }
  }
}
